<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ConsultationServlet</servlet-name>
        <servlet-class>enterprise.web_jpa_war.servlet.ConsultationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Modification</servlet-name>
        <servlet-class>enterprise.web_jpa_war.servlet.ModificationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuppressionServlet</servlet-name>
        <servlet-class>enterprise.web_jpa_war.servlet.SuppressionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsultationServlet</servlet-name>
        <url-pattern>/ConsultationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Modification</servlet-name>
        <url-pattern>/Modification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuppressionServlet</servlet-name>
        <url-pattern>/SuppressionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
